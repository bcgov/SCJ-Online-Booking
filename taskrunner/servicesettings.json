{
    "Serilog": {
        "MinimumLevel": "Information"
    },
    "Data": {
        "DefaultConnection": {
            "Provider": "sqlite",
            "ConnectionString": "data source=scj-booking.sqlite;"
        }
    },
    "AppSettings": {
        "PollingFrequencySeconds": 5,
        "LotteryEnabled": true,
        "EmailEnabled": true,
        "EmailLotteryResultEnabled": true,
        "PurgeEnabled": true,
        "PurgeLotteryContactInfoAfterDays": 4,
        "PurgeLotteryRequestsAfterDays": 40
    }
}
